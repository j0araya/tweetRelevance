{
    "initial:before": {
        "loopback#favicon": {}
    },
    "initial": {
        "./middleware/tracker": {},
        "compression": {},
        "cors": {
            "params": {
                "origin": true,
                "credentials": true,
                "maxAge": 86400
            }
        },
        "helmet#xssFilter": {},
        "helmet#frameguard": {
            "params": [
                "deny"
            ]
        },
        "helmet#hsts": {
            "params": {
                "maxAge": 0,
                "includeSubdomains": true
            }
        },
        "helmet#hidePoweredBy": {},
        "helmet#ieNoOpen": {},
        "helmet#noSniff": {},
        "helmet#noCache": {
            "enabled": false
        }
    },
    "session": {
    },
    "auth": {
        "loopback-component-oauth2#authenticate": {
            "params": {
                "session": false,
                "paths": [
                    {
                        "path": "/api/",
                        "exclude": [
                            {
                                "method": "POST",
                                "path": "Users"
                            },
                            {
                                "method": "GET",
                                "path": "Users/reset/*"
                            },
                            {
                                "method": "POST",
                                "path": "Users/reset/*"
                            },
                            {
                                "method": "POST",
                                "path": "auth"
                            },
                            {
                                "method": "GET",
                                "path": "auth"
                            },
                            {
                                "method": "GET",
                                "path": "redirect"
                            },
                            {
                                "method": "GET",
                                "path": "external/icons/*"
                            },
                            {
                                "method": "GET",
                                "path": "shared/*"
                            },
                            {
                                "method": "POST",
                                "path": "shared/*"
                            },
                            {
                                "method": "GET",
                                "path": "projects/*"
                            },
                            {
                                "method": "GET",
                                "path": "questions/*"
                            },
                            {
                                "method": "POST",
                                "path": "stripehooks"
                            },
                            {
                                "method": "GET",
                                "path": "stripehooks"
                            }


                        ]
                    }
                ]
            }
        }
    },
    "parse": {},
    "routes": {
        "loopback#rest": {
            "paths": [
                "${restApiRoot}"
            ]
        }
    },
    "files": {},
    "final": {
        "loopback#urlNotFound": {}
    },
    "final:after": {
        "strong-error-handler": {
            "params": {
                "debug": true,
                "log": true
            }
        }
    }
}
