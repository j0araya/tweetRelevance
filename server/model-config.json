{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "./api/models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./api/mixins",
      "./mixins",
      "../node_modules/loopback-ds-paginate-mixin/lib",
      "../node_modules/loopback-ds-readonly-mixin/lib",
      "../node_modules/loopback-cascade-delete-mixin",
      "../node_modules/loopback-ds-timestamp-mixin",
      "./api/mixins/loopback-mongo-aggregate-mixin/dist"
    ]
  },
  "User": {
    "dataSource": "tweet-relevance-db",
    "public": false
  },
  "userCredential": {
    "dataSource": "tweet-relevance-db",
    "public": false
  },
  "userIdentity": {
    "dataSource": "tweet-relevance-db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "tweet-relevance-db",
    "public": false
  },
  "ACL": {
    "dataSource": "tweet-relevance-db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "tweet-relevance-db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "tweet-relevance-db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },

  "user": {
    "dataSource": "tweet-relevance-db",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "remoting": {
        "sharedMethods": {
          "login": false,
          "logout": false,
          "replaceById": false
        }
      }
    }
  },
  "Email": {
    "dataSource": "emailDs"
  },
  "Plan": {
    "dataSource": "tweet-relevance-db",
    "public": true
  }
}
