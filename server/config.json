{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "hostClient": "http://localhost:3000",
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "10000kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "10000kb"
    },
    "cors": false
  },
  "logger": {
    "console": {
      "enable": true,
      "options": {
        "level": "info",
        "colorize": true
      }
    },
    "file": {
      "enable": false,
      "options": {
        "level": "info",
        "colorize": true,
        "filename": ""
      }
    },
    "mongodb": {
      "enable": false,
      "options": {
        "level": "info",
        "db": "mongodb://localhost/wenteract-db"
      }
    },
    "sentry": {
      "enable": false
    }
  }
}
